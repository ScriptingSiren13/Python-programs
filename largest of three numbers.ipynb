    "def largest_num(a,b,c):\n",
    "    if a>b and a>c:\n",
    "        return a\n",
    "    elif b>a and b>c:\n",
    "        return b;\n",
    "    else:\n",
    "        return c\n",
    "\n",
    "a=5;b=9;c=1\n",
    "print(largest_num(a,b,c))"
